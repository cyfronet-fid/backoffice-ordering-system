{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache src && prettier --list-different --cache src",
    "format": "eslint --fix --cache src && prettier --write --cache src",
    "preview": "vite preview",
    "generate-client": "openapi-ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hey-api/client-fetch": "^0.7.0",
    "@tanstack/react-router": "^1.95.5",
    "@tanstack/react-table": "^8.20.6",
    "framer-motion": "^11.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@hey-api/openapi-ts": "^0.62.0",
    "@tanstack/router-devtools": "^1.95.5",
    "@tanstack/router-plugin": "^1.95.5",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "sass": "^1.83.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  },
  "prettier": {
    "importOrderSeparation": true,
    "importOrder": ["<THIRD_PARTY_MODULES>", "^[./]"],
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
  }
}
